// Test lambda with multiple parameters

fn main() -> Int {
  // Lambda that adds two numbers
  let add = |x: Int, y: Int| -> Int {
    return x + y;
  };

  // Lambda that computes (x * y) + z
  let compute = |x: Int, y: Int, z: Int| -> Int {
    let product = x * y;
    return product + z;
  };

  let result1 = add(10, 20);        // 30
  let result2 = compute(5, 6, 10);  // 5*6 + 10 = 40

  return result1 + result2;  // 30 + 40 = 70
}
