// Test multiple lambda functions

fn main() -> Int {
  // First lambda - adds 10
  let add_ten = |x: Int| -> Int {
    return x + 10;
  };

  // Second lambda - multiplies by 2
  let double = |x: Int| -> Int {
    return x * 2;
  };

  // Third lambda - subtracts 5
  let sub_five = |x: Int| -> Int {
    return x - 5;
  };

  // Use all three lambdas
  let a = add_ten(5);    // 15
  let b = double(a);     // 30
  let c = sub_five(b);   // 25

  return c;  // Should return 25
}
