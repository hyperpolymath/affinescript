// Test importing and using Math functions

use Math::{pow, gcd, factorial};

fn main() -> Int {
  let power = pow(2, 8);        // 256
  let divisor = gcd(48, 18);    // 6
  let fact = factorial(5);      // 120
  return power + divisor + fact;  // 256 + 6 + 120 = 382
}
